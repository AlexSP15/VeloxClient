<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.velox" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Velox</name>
    <description>Proyecto Velox App Client</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Alejandro Soledad
        Paulina Villanueva
    </author>
    <icon src="./www/img/logo.png" platform="android" width="57" height="57" density="mdpi" />
    <icon src="./www/img/logo.png" platform="browser" width="57" height="57" density="mdpi" />
    <platform name="android">
        <preference name="AndroidWindowSplashScreenIconBackgroundColor" value="#ffffff" />
    </platform>
    <content src="recoleccion.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
</widget>
